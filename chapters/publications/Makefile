.PHONY: figurelist
figurelist:
	@( cd $(MAINDIR) && make --no-print-directory $@ TEXFILE=chapters/publications/publications.tex )

.PHONY: clean
clean:
	$(RM) $(foreach ext, .toc .aux .log .bbl .blg .log .lof .lot .ilg .out .glo .gls .nlo .nls .brf .ist .glg .synctex.gz .tgz .idx .ind -blx.bib .fdb_latexmk .run.xml .bcf, $(wildcard publications*$(ext)))

.PHONY: realclean
realclean: clean
	$(RM) publications.pdf publications_bare.pdf

s:
	@echo gvim publications.tex allpapers.bib > $@
	@chmod +x $@
	@echo You can now start using ./s
