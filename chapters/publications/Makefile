.PHONY: publications.pdf
publications.pdf: 
	( cd "$(MAINDIR)" && make chapters/publications/$@ )

.PHONY: bare publications_bare.pdf
bare: publications_bare.pdf
publications_bare.pdf: 
	( cd "$(MAINDIR)" && make chapters/publications/$@ )

.PHONY: image
