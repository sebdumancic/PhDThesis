.PHONY: clean
clean:
	$(RM) $(foreach ext, .toc .aux .log .bbl .blg .log .lof .lot .ilg .out .glo .gls .nlo .nls .brf .ist .glg .synctex.gz .tgz .idx .ind -blx.bib .fdb_latexmk .run.xml .bcf, $(wildcard curriculum*$(ext)))

.PHONY: realclean
realclean: clean
	$(RM) curriculum.pdf curriculum_bare.pdf

