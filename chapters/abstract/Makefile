.PHONY: abstract.pdf
abstract.pdf: 
	( cd "$(MAINDIR)" && make chapters/abstract/$@ )

.PHONY: bare abstract_bare.pdf
bare: abstract_bare.pdf
abstract_bare.pdf: 
	( cd "$(MAINDIR)" && make chapters/abstract/$@ )

.PHONY: image
image: 
	@if [ -f image/Makefile ]; then (cd image  && make); \
		else echo No Makefile found for /Users/seb/Desktop/thesis/main/image ...; fi;

.PHONY: figurelist
figurelist:
	@( cd $(MAINDIR) && make --no-print-directory $@ TEXFILE=chapters/abstract/abstract.tex )

.PHONY: clean
clean:
	$(RM) $(foreach ext, .toc .aux .log .bbl .blg .log .lof .lot .ilg .out .glo .gls .nlo .nls .brf .ist .glg .synctex.gz .tgz .idx .ind -blx.bib .fdb_latexmk .run.xml .bcf, $(wildcard abstract*$(ext)))

.PHONY: realclean
realclean: clean
	$(RM) abstract.pdf abstract_bare.pdf

s:
	@echo gvim abstract.tex allpapers.bib > $@
	@chmod +x $@
	@echo You can now start using ./s
